@model MyTunes.Models.ArtistSongsModel

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Artist</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Artist.Name)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Artist.Name)
        </dd>
    </dl>
    <h4>Songs</h4>
    @foreach (var song in Model.Songs)
    {
        <p>@song.Title</p>
    }
</div>
